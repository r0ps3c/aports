# Whether this package is the default (latest) ceph version.
# remove ceph-dev above if no
_default_ceph="no"

if [ "$_default_ceph" = yes ]; then
	provides="ceph=$pkgver-r$pkgrel"
	replaces="ceph"
fi

# secfixes:
#   16.2.10-r0:
#     - CVE-2022-0670
