From e5e1523903e3857d03328bab56672e032769226c Mon Sep 17 00:00:00 2001
From: fossdd <fossdd@pwned.life>
Date: Sat, 16 Nov 2024 23:37:10 +0100
Subject: [PATCH] Relax dotnet version dependency (4.0.404 -> 4.0.0)

This reverts commit f47d2c1f1ab668f835c3061cd498909ab6128b3b.
---
 .config/dotnet-tools.json | 2 +-
 global.json               | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/.config/dotnet-tools.json b/.config/dotnet-tools.json
index 02afa3f07..a3847dcdf 100644
--- a/.config/dotnet-tools.json
+++ b/.config/dotnet-tools.json
@@ -3,7 +3,7 @@
   "isRoot": true,
   "tools": {
     "dotnet-ef": {
-      "version": "8.0.11",
+      "version": "8.0.8",
       "commands": [
         "dotnet-ef"
       ]
diff --git a/global.json b/global.json
index c9b932026..dbf2988d5 100644
--- a/global.json
+++ b/global.json
@@ -1,6 +1,6 @@
 {
     "sdk": {
-        "version": "8.0.404",
+        "version": "8.0.0",
         "rollForward": "latestMinor"
     }
 }
-- 
2.47.0

