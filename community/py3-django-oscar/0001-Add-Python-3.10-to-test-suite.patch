diff --git a/setup.py b/setup.py
index 9ee3d8c69..1a5f1f02c 100755
--- a/setup.py
+++ b/setup.py
@@ -47,11 +47,10 @@ sorl_thumbnail_version = 'sorl-thumbnail>=12.6,<12.7'
